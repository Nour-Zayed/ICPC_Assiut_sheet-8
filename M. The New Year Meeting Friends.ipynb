    "arr = list(map(int, input().split())) \n",
    "arr1 = sorted(arr)  \n",
    "\n",
    "n = len(arr1)\n",
    "median = arr1[n // 2] \n",
    "\n",
    "summation = sum(abs(x - median) for x in arr1)\n",
    "\n",
    "print(summation)\n"
