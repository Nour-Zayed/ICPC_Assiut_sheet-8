   "source": [
    "x, y = map(int, input().split())\n",
    "\n",
    "for i in range(x):\n",
    "    if i % 2 == 0:\n",
    "        print('#' * y)\n",
    "    else:\n",
    "        if (i // 2) % 2 == 0:\n",
    "            print('.' * (y - 1) + '#')\n",
    "        else:\n",
    "            print('#' + '.' * (y - 1))"
   ]
